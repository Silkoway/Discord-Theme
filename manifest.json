{
    "name": "Discord Theme",
    "description": "Import and use custom discord themes!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://discord.com/*"
            ],
            "js": [
                "loader.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "discordtheme.js"
            ],
            "matches": [
                "https://discord.com/*"
            ]
        }
    ]
}